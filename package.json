{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Accounting Application",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js --exec babel-node --presets es2015,stage-2",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Stephens Jean-Jacques",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "@okta/jwt-verifier": "0.0.11",
    "@okta/okta-auth-js": "^1.13.0",
    "@okta/okta-react": "0.0.12",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "mongoose": "^5.0.6",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^4.8.0"
  }
}
